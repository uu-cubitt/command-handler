{
	"version": "1.0.0",
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"preserveConstEnums": true,
		"declaration": true,
		"suppressImplicitAnyIndexErrors": true,
		"outDir": "./../dist/",
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"allowUnreachableCode": false,
		"sourceMap": true
	},
	"exclude": [
		"typings/browser",
		"typings/browser.d.ts"
	]
}
